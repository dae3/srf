{
    "name": "Go Development",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "."
    },
    "features": {
        "ghcr.io/devcontainers/features/github-cli:1": {}
    },
    "containerUser": "vscode",
    "remoteUser": "vscode",
    "forwardPorts": [8080],
    "onCreateCommand": {
        "homeperm": "chown vscode /home/vscode"
    }
}

